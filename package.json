  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" ",
    "lint:fix": "npm run lint -- --fix",
    "test:unit": "jest --config ./test/unit/jest.json",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:unit:cov": "npm run test:unit -- --coverage",
    "test:unit:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest --config ./test/integration/jest-e2e.json --runInBand --detectOpenHandles --forceExit",
    "test:integration:watch": "npm run test:integration -- --watch",
    "test:integration:cov": "npm run test:integration -- --coverage",
    "test:combine:cov": "ts-node ./test/scripts/combine-coverage.ts",
    "test:open:cov": "open ./coverage/merged/lcov-report/index.html",
    "sonar:dev": "sonar-scanner -Dsonar.host.url=http://localhost:9000 -Dsonar.login=${SONAR_TOKEN}"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
